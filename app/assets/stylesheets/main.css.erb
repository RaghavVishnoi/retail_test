body {
  margin: 0px;
  padding: 0px;
}
.clearfix{
display: block;
}
.clearfix:after {
  content: " ";
  display: block;
  clear: both;
  visibility: hidden;
  height: 0;
  line-height: 0;
}

#nav-bar-wrapper {
  margin: 0 auto;
  width: 750px;
}

#nav-bar {
  padding: 0px;
  float: right;
  margin: 0px;
}

#nav-bar li {
  display: inline;
  margin: 0px 5px;
}

#nav-bar-container {
  background-color: black;
  padding: 2px 0px 2px 0px;
  position: fixed;
  top: 0px;
  width: 100%;
  z-index: 1
}

#nav-bar a {
  color: white;
  text-decoration: none;
}

#page-container {
  position: relative;
  top: 25px;
  width: 750px;
  margin: 0 auto;
}

#new-button {
  padding: 5px 8px;
  text-decoration: none;
  background-color: rgb(116, 116, 235);
  color: white;
  display: inline-block;
  margin: 8px 0px 10px 0px;
}

.user-form {
  width: 500px;
  margin: 0 auto;
}

.user-form .form-field {
  margin: 4px 0px 8px 0px;
}

.user-form .form-field input, .user-form .form-field .field-value {
  width: 250px;
  height: 25px;
}

.form-field select {
  width: 250px;
}

.user-form .form-field label, .user-form .form-field .field-name {
  width: 150px;
  text-align: left;
  display: inline-block;
}

#form-title {
  text-align: center;
}

.field_with_errors {
  display: inline;
}

input[type="submit"] {
  background-color: rgb(65, 187, 65);
  color: white;
  font-size: 15px;
  border: none;
  padding: 4px 5px;
  cursor: pointer;
  margin-top: 8px;
}

.error-messages-container {
  padding: 1px 0px 1px 10px;
  margin-bottom: 25px;
  border: 1px solid rgb(235, 140, 140);
}

.error-messages-container h4 {
  margin: 2px 0px 2px 0px;
}

.error-messages-container ul {
  margin: 0px;
}

.password-form {
  width: 255px;
}

.organization-logo {
  max-width: 100px;
  max-height: 100px;
  margin-bottom: 8px;
}

.organization-nav-container {
  text-align: center;
}

.organization-nav-container .nav-link {
  display: inline-block;
}

.organization-nav-container .nav-link a {
  padding: 5px;
  display: block;
}

#welcome-msg { text-align: center; }

.weekly-off-field { width: 500px !important; }

.hidden { display: none !important; }

.image-container img { 
  max-width: 100px;
  max-height: 100px;
}

.image-container a {
  float: right;
}

.screen-name, .components-container, .component, .layout-element-container, .ui-elements-container, .ui-element, .elements-container, .layout-elements-container { border: 1px solid black; margin: 2px;}

.component, .ui-element, .layout-elements-container {
  margin: 10px;
}

