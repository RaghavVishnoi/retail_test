.box_menu{padding: 10px 0px;}
.box_menu .box{ 
  padding: 10px;
  width:20%; 
  max-width: 150px;
  margin: 10px 10px 20px 10px;  
  float: left;
  text-align: center;
  min-height: 220px;
}
.box_menu .box span
{
  border:solid 1px #CCC; 
  width:100%; max-width: 150px;
  min-height: 150px;
  display: block;
  margin-bottom: 15px;
  transition: 1s;
  cursor: pointer;
  background: #FFF;
}
.box_menu .box i
{
  font-size: 70px;
  color: #CCC;
  margin: 40px 0px;
  transition: 1s;
  display: inline-block;
}
.box_menu .box i img
{ width:60%;}
.box_menu .box span:hover{border:solid 1px #08af91; }
.box_menu .box span:hover i{ color: #04d8b3;
-webkit-transform: rotateY(180deg);
-webkit-transform-style: preserve-3d;
transform: rotateY(180deg);
transform-style: preserve-3d;}
/*===END===*/



