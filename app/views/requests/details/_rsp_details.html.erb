

<section class="comments_sectn">
  <div class="row"> 
    <div class="col-lg-12">
      <div class="main-box clearfix">
        <div class="clearfix">
          <div class="common_page gsb_page">
            <h2>RSP/LFR/Other details</h2>

          <%if(@request.is_rsp == true)%>
            <div class="comments_blk">  
             <div class="row">
                <div class="col-md-5">
                  Is RSP?
                </div>
                <div class="col-md-4">
                  <div class="blue">
                    <%=to_bool(@request.is_rsp)%>
                  </div>
                </div>
              </div>           
              <div class="row">
                <div class="col-md-5">
                  RSP Name
                </div>
                <div class="col-md-4">
                  <div class="blue">
                    <%=@request.rsp_name%>
                  </div>
                </div>
              </div>
               
              <div class="row">
                <div class="col-md-5">
                  RSP Contact Number
                </div>
                <div class="col-md-4">
                  <div class="blue">
                    <%=@request.rsp_mobile_number%>
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-md-5">
                  RSP App Id
                </div>
                <div class="col-md-4">
                  <div class="blue">
                    <%=@request.rsp_app_user_id%>
                  </div>
                </div>
              </div>
             
            </div>
          

            <%elsif @request.lfr_name != nil%>
            <div class="comments_blk">
            <div class="row">
                <div class="col-md-5">
                  Is RSP?
                </div>
                <div class="col-md-4">
                  <div class="blue">
                    <%=to_bool(@request.is_rsp)%>
                  </div>
                </div>
              </div>      
             <div class="row">
                <div class="col-md-5">
                  LFR Name
                </div>
                <div class="col-md-4">
                  <div class="blue">
                    <%=@request.lfr_name%>
                  </div>
                </div>
              </div>           
              <div class="row">
                <div class="col-md-5">
                 LFR Contact Number
                </div>
                <div class="col-md-4">
                  <div class="blue">
                    <%=@request.lfr_phone%>
                  </div>
                </div>
              </div>
               
              <div class="row">
                <div class="col-md-5">
                  LFR App User ID
                </div>
                <div class="col-md-4">
                  <div class="blue">
                    <%=@request.lfr_app_user_id%>
                  </div>
                </div>
              </div>
               
             
            </div>
            
             <%elsif @request.other_name != nil%>
            <div class="comments_blk">
            <div class="row">
                <div class="col-md-5">
                  Is RSP?
                </div>
                <div class="col-md-4">
                  <div class="blue">
                    <%=to_bool(@request.is_rsp)%>
                  </div>
                </div>
              </div>      
             <div class="row">
                <div class="col-md-5">
                  Other Name
                </div>
                <div class="col-md-4">
                  <div class="blue">
                    <%=@request.other_name%>
                  </div>
                </div>
              </div>           
              <div class="row">
                <div class="col-md-5">
                 Other Contact Number
                </div>
                <div class="col-md-4">
                  <div class="blue">
                    <%=@request.other_phone%>
                  </div>
                </div>
              </div>
               
              <div class="row">
                <div class="col-md-5">
                  Other Address
                </div>
                <div class="col-md-4">
                  <div class="blue">
                    <%=@request.other_address%>
                  </div>
                </div>
              </div>
               
             
            </div>
            <%end%>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>  
