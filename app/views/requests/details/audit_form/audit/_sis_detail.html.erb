<section class="comments_sectn">
	<div class="row">	
		<div class="col-lg-12">
			<div class="main-box clearfix">
				<div class="clearfix">
					<div class="common_page gsb_page">
						<h2>SIS Details</h2>
						<div class="comments_blk">
							<div class="row">
								<div class="col-md-5">
									SIS present in store 
								</div>
								<div class="col-md-4">
									<div class="blue">
										<%=to_bool(@request.shop_audit.sis_present_in_store)%>
									</div>
								</div>
							</div>
							<%if @request.shop_audit.sis_present_in_store == true%>
							<div class="row">
								<div class="col-md-5">
									Type of SIS
								</div>
								<div class="col-md-4">
									<div class="blue">
										<%=@request.shop_audit.sis_type%> 
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-5">
									Backwall Number
								</div>
								<div class="col-md-4">
									<div class="blue">
										<%=@request.shop_audit.back_wall_nos%>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-5">
									Glass counter number 
								</div>
								<div class="col-md-4">
									<div class="blue">
										<%=@request.shop_audit.glass_counter_no%> 
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-5">
									Experience counter number
								</div>
								<div class="col-md-4">
									<div class="blue">
										<%=@request.shop_audit.experience_counter_no%>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-5">
									SIS condition
								</div>
								<div class="col-md-4">
									<div class="blue">
										<%=@request.shop_audit.sis_condition%> 
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-5">
									Needs
								</div>
								<div class="col-md-4">
									<div class="blue">
										<%=@request.shop_audit.sis_needs%> 
									</div>
								</div>
							</div>
							<%end%>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>	