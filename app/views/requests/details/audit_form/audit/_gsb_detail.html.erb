<section class="comments_sectn">
	<div class="row">	
		<div class="col-lg-12">
			<div class="main-box clearfix">
				<div class="clearfix">
					<div class="common_page gsb_page">
						<h2>GSB Details</h2>
						<div class="comments_blk">
							<div class="row">
								<div class="col-md-5">
									GSB present in store 
								</div>
								<div class="col-md-4">
									<div class="blue">
									<%=to_bool(@request.shop_audit.gsb_present_at_store)%>
									</div>
								</div>
							</div>
							<%if @request.shop_audit.gsb_present_at_store == true%>
							<div class="row">
								<div class="col-md-5">
									Number of GSB present
								</div>
								<div class="col-md-4">
									<div class="blue">
										<%=@request.shop_audit.no_of_gsb%>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-5">
									Type of GSB
								</div>
								<div class="col-md-4">
									<div class="blue">
										<%=@request.shop_audit.gsb_type_installed%>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-5">
									LOGO
								</div>
								<div class="col-md-4">
									<div class="blue">
										<%=@request.shop_audit.gsb_type_logo%> 
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-5">
									Position
								</div>
								<div class="col-md-4">
									<div class="blue">
										<%=@request.shop_audit.gsb_position%>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-5">
									GSB Condition
								</div>
								<div class="col-md-4">
									<div class="blue">
										<%=@request.shop_audit.gsb_condition%>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-5">
									Size
								</div>
								<div class="col-md-4">
									<div class="blue">
										<%=@request.shop_audit.gsb_size%>
									</div>
								</div>
							</div>
							<%end%>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>	