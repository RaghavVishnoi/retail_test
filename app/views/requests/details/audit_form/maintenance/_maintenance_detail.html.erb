<section class="comments_sectn">
	<div class="row">	
		<div class="col-lg-12">
			<div class="main-box clearfix">
				<div class="clearfix">
					<div class="common_page gsb_page">
						<h2>Maintenance Details</h2>
						<div class="comments_blk">
							<div class="row">
								<div class="col-md-5">
									Maintenance done?
								</div>
								<div class="col-md-4">
									<div class="blue">
									<%=@request.shop_audit.maintenance_done%>
									</div>
								</div>
							</div>
							<%if @request.shop_audit.maintenance_done == true%>
							<div class="row">
								<div class="col-md-5">
									Maintenance done on item
								</div>
								<div class="col-md-4">
									<div class="blue">
										 <%=@request.shop_audit.maintenance_done_on%>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-5">
									Consumable used
								</div>
								<div class="col-md-4">
									<div class="blue">
										 <%=@request.shop_audit.consumnables_used%>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-5">
									Type of issue
								</div>
								<div class="col-md-4">
									<div class="blue">
										 <%=@request.shop_audit.type_of_issue%>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-5">
									Problem solved
								</div>
								<div class="col-md-4">
									<div class="blue">
										 <%=@request.shop_audit.problem_solved%>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-5">
									Escalate to supervisor
								</div>
								<div class="col-md-4">
									<div class="blue">
										 <%=@request.shop_audit.escalate_to_tl%>
									</div>
								</div>
							</div>
							<%end%>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>	