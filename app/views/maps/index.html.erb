  function map_data(
    from_date,till_date,state,request_type,status_type){
    $('.loader').attr("style","display:block");
     $.ajax({
                 url: "/maps/map_data",   
                 type: "POST",
                 data: {  from_date : from_date,
                         till_date : till_date,
                         state : state,
                         request_type : request_type,
                         status_type: status_type

                }, 
