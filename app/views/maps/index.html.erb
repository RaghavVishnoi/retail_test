                      lln.push({lat: response[i][3],lon: response[i][4],zoom: 5,icon: icon,reqId: response[i][5],reqType: response[i][0]})
                  }
                  
                     
                  }//end of main if
                        
              }//end of for loop
 
                    initMap(lln);
 
                   
                

            },//end of success function
                error: function() {
                         alert("Ajax error!")
                }
        });
