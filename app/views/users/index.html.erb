<table>
  <tr>
    <td>Email</td>
    <td>Name</td>
    <td></td>
    <td></td>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td>
        <%= user.email %>
      </td>
      <td>
        <%= user.name %>
      </td>
      <td>
        <%= link_to "Edit", [:edit, user] %>
      </td>
      <td>
        <%= link_to "Delete", [user], method: :delete, data: { confirm: "Are you sure?" } %>
      </td>
    </tr>
  <% end %>
</table>